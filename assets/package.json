    "history": "^4.7.2",
    "react": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "save-dev": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
